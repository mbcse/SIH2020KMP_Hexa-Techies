                <input class="input-md emailinput form-control" id="applicantEmail" value="abc@gmail.com" name="applicantEmail" placeholder="Your current email address" style="margin-bottom: 10px" type="email" />
            </div>     
        </div>

        <div id="div_id_nationality" class="form-group required">
            <label for="applicantNationality" class="control-label  requiredField">Nationality<span class="asteriskField">*</span> </label>
            <div class="controls  "> 
                <input class="input-md   form-control" id="applicantNationality" value="Indian" name="applicantNationality" placeholder="Enter your Nationality" style="margin-bottom: 10px" type="text" />
            </div>
