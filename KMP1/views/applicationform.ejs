        <div id="aerodromeSite" class="form-group required"> 
            <label for="aerodromeSite" class="control-label  requiredField"> Site of Aerodrome </label>  
            <div class="controls  "> 
                <input class="input-md   form-control" id="aerodromeSite" value="Greater noida" name="aerodromeSite" placeholder="" style="margin-bottom: 10px" type="text" />
            </div>
        </div>

        <div id="aerodromeState_District" class="form-group required"> 
            <label for="aerodromeState_District" class="control-label   requiredField"> State / District in which situated  </label>
