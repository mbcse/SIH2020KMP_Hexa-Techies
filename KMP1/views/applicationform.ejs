<div class="panel-body" >

  
            

    <form  class="form-horizontal" id="NewApplication" method="post" >
        
        <div id="" class="form-group required">
            <label for="applicantName" class="control-label     requiredField">Applicant Full Name<span class="asteriskField">*</span> </label>
            <div class="controls ">
                <input class="form-control" id="applicantName"  maxlength="30" name="username" value="Test1" placeholder="Full Name in Capital Letters" style="margin-bottom: 10px" type="text" />
            </div>
        </div>

        <div class="form-group">
            <label for="applicantAddress">Address<span class="asteriskField">*</span></label>
            <input type="text" class="form-control" id="applicantAddress" name="applicantAddress" value="abcdefghigklmn" placeholder="1234 Main St">
        </div>

        <div class="form-group">
            <label for="applicantAddress2">Address 2</label> 
            <input type="text" class="form-control" id="applicantAddress2" name="applicantAddress2" value="abcdefghijklmn" placeholder="Apartment, studio, or floor">
        </div>

        <div class="form-row">
            <div class="form-group ">
                <label for="applicantCity">City<span class="asteriskField">*</span></label> 
                <input type="text" class="form-control" value="New Delhi" id="applicantCity" name="applicantCity">
            </div>
            <div class="form-group ">
                <label for="applicantState">State<span class="asteriskField">*</span></label> 
                <input type="text" class="form-control" value="Delhi" id="applicantState" name="applicantState">
            </div>
            <div class="form-group ">
                <label for="applicantZip">Zip<span class="asteriskField">*</span></label> 
                <input type="text" class="form-control" value="abcdefghijklmn" id="applicantZip" name="applicantZip">
            </div>
        </div>
        
        <div class="form-group >
            <label for="applicantPhone">Phone Number:<span class="asteriskField">*</span></label> 
            <input type="tel" id="applicantPhone" name="applicantPhone" value="999-000-9999" pattern="[0-9]{3}-[0-9]{3}-[0-9]{4}" required>    
        </div>

        <div id="div_id_email" class="form-group required">
            <label for="applicantEmail" class="control-label   requiredField"> E-mail<span class="asteriskField">*</span> </label>
